{
  "expo": {
    "name": "InnosoftAPP",
    "slug": "InnosoftAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/home/logo-innosoft.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2C4365"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/home/logo-innosoft.png",
        "backgroundColor": "#2C4365"
      },
      "package": "innosoft.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    }
  }
}
